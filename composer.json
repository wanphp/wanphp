{
  "name": "wanphp/slim",
  "description": "Wan php Slim Framework for development",
  "keywords": [
    "microframework"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "火子",
      "email": "wanphp@outlook.com",
      "homepage": "http://www.wanphp.com/"
    }
  ],
  "require": {
    "monolog/monolog": "^2.2",
    "predis/predis": "^1.1.0",
    "wanphp/libray-slim": "^1.0.0",
    "wanphp/libray-mysql": "^1.1.0",
    "wanphp/plugins-weixin": "^1.1.0",
    "wanphp/plugins-oauth2-authorization": "^1.0.0",
    "wanphp/plugins-oauth2-resource": "^1.0.0",
    "almasaeed2010/adminlte": "^3.0",
    "defuse/php-encryption": "^2.2",
    "league/oauth2-server": "^8.2",
    "bacon/bacon-qr-code": "^2.0.0",
    "zircote/swagger-php": "^3.1",
    "swagger-api/swagger-ui": "^3.50",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true,
    "allow-plugins": {
      "wanphp/installers": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  }
}
