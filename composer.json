{
  "name": "wanphp/slim",
  "description": "Slim 微服务接口开发",
  "keywords": [
    "microframework"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "火子",
      "email": "yxyong@foxmail.com",
      "homepage": "http://www.wanphp.com/"
    }
  ],
  "require": {
    "monolog/monolog": "^2.0",
    "php": "^7.4",
    "catfan/medoo": "^1.7",
    "php-di/php-di": "^6.1",
    "slim/psr7": "^1.2",
    "slim/slim": "^4.5",
    "defuse/php-encryption": "^2.2",
    "league/oauth2-server": "^8.1",
    "zircote/swagger-php": "^3.0",
    "swagger-api/swagger-ui": "^3.34",
    "guzzlehttp/guzzle": "^7.1",
    "wechatpay/wechatpay-guzzle-middleware": "^0.2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}
