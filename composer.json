{
  "name": "wanphp/slim",
  "description": "Slim 微服务接口开发",
  "keywords": [
    "microframework"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "火子",
      "email": "yxyong@foxmail.com",
      "homepage": "http://www.wanphp.com/"
    }
  ],
  "require": {
    "monolog/monolog": "^2.2",
    "php": "^8.0",
    "catfan/medoo": "^1.7",
    "php-di/php-di": "^6.3",
    "slim/psr7": "^1.3",
    "slim/slim": "^4.7",
    "defuse/php-encryption": "^2.2",
    "league/oauth2-server": "^8.2",
    "zircote/swagger-php": "^3.1",
    "swagger-api/swagger-ui": "^3.44",
    "guzzlehttp/guzzle": "^7.1",
    "wechatpay/wechatpay-guzzle-middleware": "0.2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}
